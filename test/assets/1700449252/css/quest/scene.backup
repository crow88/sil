.cnt-movie {
  display: none;
  position: absolute;
  text-align: center;
  top: 0px;
  width: 100%;
  height: 450px;
  z-index: 250001
}

.cnt-movie .prt-movie {
  position: absolute;
  top: 50%;
  left: 0px;
  right: 0px;
  margin: 0 auto;
